pipeline {
  agent any
  stages {
    stage('Git') {
      agent any
      steps {
        sh 'echo "Git command"'
      }
    }
    stage('Docker') {
      agent any
      steps {
        sh 'echo "Docker command"'
      }
    }
    stage('Kubernetes') {
      agent any
      steps {
        sh 'echo "Kubernetes command"'
      }
    }

  }
}